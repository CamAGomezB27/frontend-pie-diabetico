# Configuración de EmailJS para el formulario de soporte

# ID del servicio de EmailJS (Obtener desde https://dashboard.emailjs.com/)
VITE_EMAILJS_SERVICE_ID=your_service_id

# ID de la plantilla de EmailJS para mensajes de soporte (Configurar en EmailJS)
VITE_EMAILJS_TEMPLATE_ID=your_template_id

# Clave pública de EmailJS (Obtener desde https://dashboard.emailjs.com/)
VITE_EMAILJS_PUBLIC_KEY=your_public_key

# Correo donde se recibirán los mensajes de soporte
VITE_EMAILJS_TO_EMAIL=your_support_email

# ID de la plantilla para confirmaciones de EmailJS
VITE_EMAILJS_TEMPLATE_ID_CONFIRMATION=your_confirmation_template_id
